{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SerilogLogging": {
    "MinimumLogLevel": "Information",
    "EnableFileLogging": true,
    "EnableConsoleLogging": true,
    "FilePath": "logs/file.log"
  },
  "AllowedHosts": "*"
}
